{
  "name": "wdio-vscode-service",
  "version": "1.1.0",
  "description": "A service to test VSCode extensions from end to end",
  "author": "Christian Bromann <mail@bromann.dev>",
  "homepage": "https://github.com/webdriverio-community/wdio-vscode-service",
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": ">=14.0.0"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:ts": "tsc -b .",
    "build:compile": "pkg --target win --out-path ./dist/chromium ./dist/chromium/index.js",
    "build:chmod": "chmod +x ./dist/chromium/index.js",
    "ci": "run-s lint build test",
    "clean": "rimraf ./node_modules yarn-lock.yaml ./dist",
    "docs": "typedoc",
    "prepare": "husky install",
    "release": "release-it --github.release",
    "release:ci": "npm run release -- --ci --npm.skipChecks --no-git.requireCleanWorkingDir",
    "release:patch": "npm run release -- patch",
    "release:minor": "npm run release -- minor",
    "release:major": "npm run release -- major",
    "test": "run-s test:*",
    "test:setup": "node ./.github/scripts/link.mjs",
    "test:lint": "eslint src test example",
    "test:e2e": "wdio run ./test/wdio.conf.ts",
    "watch": "yarn run build:ts --watch"
  },
  "keywords": [
    "webdriverio",
    "wdio",
    "wdio-service",
    "vscode",
    "tests"
  ],
  "dependencies": {
    "@types/ws": "^8.5.3",
    "@vscode/test-electron": "^2.1.3",
    "@wdio/logger": "^7.17.3",
    "clipboardy": "^2.3.0",
    "decamelize": "5.0.1",
    "download": "^8.0.0",
    "get-port": "5.1.1",
    "slash": "^3.0.0",
    "tmp-promise": "^3.0.3",
    "undici": "^4.15.1",
    "wdio-chromedriver-service": "^7.3.2",
    "ws": "^8.5.0",
    "yargs-parser": "^21.0.1"
  },
  "peerDependencies": {
    "webdriverio": "^7.19.2"
  },
  "peerDependenciesMeta": {
    "webdriverio": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/chromedriver": "^81.0.1",
    "@types/download": "^8.0.1",
    "@types/node": "^17.0.21",
    "@types/vscode": "^1.66.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "@wdio/cli": "^7.19.1",
    "@wdio/local-runner": "^7.19.1",
    "@wdio/mocha-framework": "^7.19.1",
    "@wdio/spec-reporter": "^7.19.1",
    "@wdio/types": "^7.17.3",
    "chromedriver": "^99.0.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-base-typescript": "^1.0.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-wdio": "^7.18.1",
    "husky": "^7.0.4",
    "npm-run-all": "^4.1.5",
    "pkg": "^5.5.2",
    "release-it": "^14.12.5",
    "rimraf": "^3.0.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "files": [
    "dist/*"
  ]
}
